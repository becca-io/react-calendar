{
  "name": "h6s",
  "private": true,
  "description": "Serve headless component development kit for React production apps",
  "keywords": [
    "headless",
    "react",
    "cdk",
    "calendar",
    "fullcalendar",
    "react-calendar",
    "table",
    "complex-table",
    "merge table",
    "react-table"
  ],
  "author": "JaeYeopHan(Jbee)",
  "organization": "h6s-dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/h6s-dev/h6s"
  },
  "homepage": "https://h6s.dev",
  "license": "MIT",
  "lint-staged": {
    "packages/**/*.ts": [
      "eslint -c .eslintrc '{src}/**/*.{js,jsx,ts,tsx}' --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@babel/core": "7.16.5",
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@mdx-js/react": "1.6.22",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.2",
    "@semantic-release/npm": "8.0.3",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@storybook/addon-actions": "6.4.9",
    "@storybook/addon-docs": "6.4.9",
    "@storybook/addon-essentials": "6.4.9",
    "@storybook/addon-links": "6.4.9",
    "@storybook/react": "6.4.9",
    "@testing-library/cypress": "8.0.2",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "^27.0.3",
    "@types/react": "17.0.38",
    "@types/testing-library__jest-dom": "5.14.2",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@typescript-eslint/parser": "5.8.0",
    "@yarnpkg/esbuild-plugin-pnp": "2.0.0",
    "@yarnpkg/pnpify": "3.1.1-rc.8",
    "acorn": "8.7.0",
    "all-contributors-cli": "6.20.0",
    "concurrently": "6.5.1",
    "cypress": "9.2.0",
    "cz-conventional-changelog": "3.3.0",
    "date-fns": "2.27.0",
    "esbuild": "0.14.8",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "7.0.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "25.3.0",
    "eslint-plugin-jest-dom": "3.9.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-testing-library": "5.0.1",
    "framer-motion": "5.5.5",
    "husky": "7.0.4",
    "jest": "27.4.5",
    "lint-staged": "12.1.4",
    "prettier": "2.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2",
    "rimraf": "3.0.2",
    "semantic-release": "18.0.1",
    "semantic-release-monorepo": "7.0.5",
    "ts-jest": "27.1.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  },
  "workspaces": [
    "packages/*",
    "examples/*",
    "website"
  ],
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "lint": "yarn workspaces foreach -pt --from '@h6s/*' run lint",
    "test": "yarn workspaces foreach -pt --from '@h6s/*' run test",
    "typecheck": "yarn workspaces foreach -pt --from '@h6s/*' run typecheck",
    "semantic-release": "yarn workspaces foreach --from '@h6s/*' run semantic-release",
    "build": "yarn workspaces foreach -ptR --from '@h6s/*' run build",
    "build:storybook": "build-storybook",
    "build:website": "yarn workspaces foreach --include 'website' run build",
    "build:examples": "yarn workspaces foreach -ptR --from '@h6s-examples/*' run build",
    "postinstall": "husky install"
  },
  "packageManager": "yarn@3.1.1"
}
