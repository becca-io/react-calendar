{
  "name": "@h6s/calendar",
  "version": "0.0.1",
  "main": "src/index.ts",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "main": "cjs/index.js",
    "types": "dist/index.d.ts",
    "module": "dist/index.js"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "require": "./cjs/index.js"
    }
  },
  "scripts": {
    "prepack": "yarn build",
    "prebuild": "rimraf dist cjs",
    "build:type": "yarn run -T tsc --emitDeclarationOnly --declaration",
    "build": "yarn build:type && node ../../scripts/build.js",
    "lint": "yarn run -T eslint '{pages,src}/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "typecheck": "yarn run -T tsc",
    "test": "yarn run -T jest --config ../../jest.config.js",
    "test:cov": "yarn test --coverage",
    "test:watch": "yarn test --watch",
    "semantic-release": "semantic-release"
  },
  "files": [
    "dist",
    "esm"
  ],
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@storybook/react": "^6.4.9",
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.4",
    "@types/react": "^17.0.0",
    "cypress": "^7.2.0",
    "date-fns": "^2.16.1",
    "husky": "^4.3.6",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1"
  },
  "peerDependencies": {
    "date-fns": ">= 2",
    "react": "^16.8.0 || ^17.0.0"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  },
  "packageManager": "yarn@3.1.1"
}
