{
  "name": "@h6s/calendar",
  "version": "1.0.0-alpha.1",
  "main": "src/index.ts",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "main": "cjs/index.js",
    "types": "dist/index.d.ts",
    "module": "dist/index.js"
  },
  "scripts": {
    "prepack": "yarn build",
    "prebuild": "rimraf dist cjs",
    "build:type": "yarn run -T tsc --emitDeclarationOnly --declaration",
    "build": "yarn build:type && node ../../scripts/build.js",
    "lint": "yarn run -T eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "typecheck": "yarn run -T tsc",
    "test": "yarn run -T jest --config ../../jest.config.js",
    "test:cov": "yarn test --coverage",
    "test:watch": "yarn test --watch",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@storybook/react": "^6.4.9",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.5",
    "@types/react": "^17.0.38",
    "cypress": "^9.2.0",
    "date-fns": "^2.28.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2"
  },
  "peerDependencies": {
    "date-fns": ">= 2",
    "react": "^16.8.0 || ^17.0.0"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  }
}
